<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>

    <link rel="stylesheet" href="/css/usercontact.css" />
  </head>
  <body>
    <%- include('./layouts/sidebar') %>
    <h1>User Data</h1>
    <table>
      <thead>
        <tr>
          <th>Sl No</th>
          <th>Name</th>
          <th>Email</th>
          <th>Subject</th>
          <th>Message</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((user, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.subject %></td>
          <td><%= user.message %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
